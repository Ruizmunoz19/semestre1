#1 Cual es el resultado de las siguientes operaciones

a = (5 + 5) * 5
print(a)

b = 5 + (5*5)
print(b)

c = 5 % 2
print(c)

d = (5 - (6/3)) * 2
print(d)

#2 si a = 7 y b = 3, calcula el resultado de cada una de las siguientes opoeraciones

a = 7
b = 3

a = a != b
print(a)

b = a == b
print(b)

c = a > b
print(c)

d = (a+b) < 2
print(d)

e = (a > 7), (b == 3)
print(e)

f = (a < 7), (b == 3)
print(f)

g = (a < 7), (b == 3)
print(g)

#3 Prueba Debuging pendiente

#4 En un curso de “fundamentos de algoritmos”....

taller1 = float(input("Ingrese la nota del taller 1: "))
taller2 = float(input("Ingrese la nota del taller 2: "))
cuestionario1 = float(input("Ingrese la nota del cuestionario 1: "))
cuestionario2 = float(input("Ingrese la nota del cuestionario 2:"))
proyecto_final = float(input("Ingrese la nota del proyecto final"))

nota = taller1*0.2 + taller2*0.15 + cuestionario1*0.2 + cuestionario1*0.15 + cuestionario2*0.1 + proyecto_final*0.33
print("La nota definitiva es: ", nota)

#5 convertir metros a millas
metros = float(input("Ingrese la distancia en metros: "))
millas = metros / 1609344
print("La distancia en millas es: " + str(millas))

#6 incremento salario minimo del 4.2%
salario = float(1300000)
porcentaje = 4.2
incremento = porcentaje * salario / 100 + salario
print("El salario para el 2025 es de: ", (incremento))

#7 pesos colombianos a dolares
peso = float(input("ingrese la cantidad en pesos: "))
dolar = float(input("ingrese el precio del dolar"))
conversion = peso / dolar
print("esa cantidad de pesos en dolares es: ", conversion)

